<?xml version="1.0"?>
<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <remove src="mage/gallery/gallery.css" />
    </head>
    <body>
        <attribute name="itemtype" value="" />
        <attribute name="itemscope" value="" />
        <referenceContainer name="app.data">
            <block class="App\Component\Block\Product" name="product.index.data" template="Magento_Catalog::data/product_data.phtml" />
        </referenceContainer>
        <referenceContainer name="app.modules">
            <block class="Magento\Framework\View\Element\Template" name="product.index.modules" template="Magento_Catalog::modules/product_index.phtml" />
        </referenceContainer>
    </body>
</page>